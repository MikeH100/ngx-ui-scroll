{
  "name": "ngx-ui-scroll-app",
  "version": "0.0.0",
  "description": "Angular Demo App for ngx-ui-scroll",
  "main": "./index.html",
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:env": "node env",
    "start:api": "node ./server/index.js",
    "lint": "eslint . --ext .ts",
    "test": "npm run lint && ng test scroller",
    "preinstall": "cd server && npm install",
    "build:lib": "ng build scroller --configuration production",
    "build:lib:watch": "ng build scroller --watch",
    "build:demo": "ng build demo --configuration production",
    "build:demo:watch": "ng build demo --watch --configuration development",
    "deploy:app": "npm run build-app && firebase deploy",
    "pack:install": "npm run build:lib && npm pack ./dist/scroller && npm install ngx-ui-scroll-3.0.0-alpha.tgz --no-save",
    "pack:start": "npm run pack:install && npm start",
    "publish:lib": "npm run build:lib && npm publish ./dist/scroller",
    "publish:lib:next": "npm run build:lib && npm publish --tag next ./dist/scroller"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "ngx-bootstrap": "^5.6.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "vscroll": "^1.4.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.14",
    "@angular/cli": "~12.2.10",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-spec-reporter": "^0.0.32",
    "ng-packagr": "^12.1.1",
    "puppeteer": "^13.0.0",
    "typescript": "~4.3.5"
  }
}
