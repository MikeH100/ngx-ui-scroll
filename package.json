{
  "name": "ngx-ui-scroll-app",
  "version": "0.0.0",
  "description": "Angular Demo App for ngx-ui-scroll",
  "main": "./index.html",
  "scripts": {
    "start": "ng serve --proxy-config ./demo/proxy.conf.json",
    "start:env": "node env",
    "start:server": "node ./server/index.js",
    "lint": "eslint . --ext .ts",
    "test:lib": "ng test scroller",
    "test": "npm run lint && npm run test:lib",
    "preinstall": "cd server && npm install",
    "copy": "cp README.md LICENSE dist/scroller",
    "build:lib": "ng build scroller --configuration production && npm run copy",
    "build:lib:watch": "ng build scroller --watch",
    "build:demo": "ng build demo --configuration production",
    "build:demo:watch": "ng build demo --watch --configuration development",
    "deploy:demo": "npm run build:demo && firebase deploy",
    "pack:lib": "npm run build:lib && npm pack ./dist/scroller",
    "publish:lib": "npm run build:lib && npm publish ./dist/scroller",
    "publish:lib:next": "npm run build:lib && npm publish --tag next ./dist/scroller"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "ngx-bootstrap": "^8.0.0-RC.5",
    "rxjs": "~7.5.1",
    "tslib": "^2.3.1",
    "vscroll": "^1.4.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.1",
    "@angular/cli": "~13.1.1",
    "@angular/compiler-cli": "~13.1.1",
    "@types/jasmine": "~3.10.3",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.10",
    "karma-chrome-launcher": "~3.1.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~4.0.1",
    "ng-packagr": "^13.1.1",
    "puppeteer": "^13.0.1",
    "typescript": "~4.5.4"
  }
}
